<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
  <test name="Personal Loan">
    <classes>
      <class name="TstNGFrm.NGFrm.day1">
       <methods>
             <exclude name="demo_day1D"/>
        </methods>
      </class> 
      <class name="TstNGFrm.NGFrm.day2"/>
    </classes>
  </test> 
  
  <test name="Mobile Loan">
    <classes>
      <class name="TstNGFrm.NGFrm.day4">
        <methods>
             <exclude name="mobile.*"/>
        </methods>
      </class>
   </classes>
  </test> 
  
</suite>
